APP_NAME=GolangStarterKit
APP_VERSION=v1
APP_ENV=dev
APP_HOST=localhost
APP_PORT=8080

# Database
DB_CONN=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASS=yourpassword
DB_NAME=mydb
DB_PATH=./sqlite.db

# JWT Config
JWT_SECRET=supersecretkeyyoushouldchange
JWT_EXPIRE_HOURS=24

# Redis Config
REDIS_CONN=redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_USER=redis
REDIS_PASS=secret

# Migration (optional)
MIGRATION_DIR=./migrations

# Logging
LOG_LEVEL=debug

# Timezone
TIMEZONE=Asia/Jakarta

# CORS
CORS_ALLOW_ORIGIN=*
CORS_ALLOW_METHODS=GET,POST,PUT,PATCH,DELETE,OPTIONS
CORS_ALLOW_HEADERS=Content-Type,Authorization,Accept,Origin,X-Requested-With
